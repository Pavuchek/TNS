<Window x:Class="CRM.Pages.CheckEquipment"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CRM.Pages"
        mc:Ignorable="d"
        Title="Проверка оборудования" Height="200" Width="400">
    <Grid>
        <ProgressBar x:Name="pBar" Height="20" Width="300" VerticalAlignment="Top" Margin="0,35,0,0" Minimum="0" Maximum="100" ValueChanged="pBar_ValueChanged"></ProgressBar>
        <Button x:Name="btnStart" Content="Проверить оборудование" Height="20" Width="150" Click="btnStart_Click"></Button>
    </Grid>
</Window>
